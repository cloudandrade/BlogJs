<center><h1>{{categoria.nome}}</h1></center>
{{#each postagens}}
<div class="card mt-2">
    <div class="card-body">
        <h3>{{titulo}}</h3>
        <h5>{{descricao}}</h5>
        <a href="/postagem/{{slug}}"><button class="btn btn-primary">Leia Mais</button></a>
        
        <hr>
        <small>  Data de publicação: {{data}}</small>
    </div>
</div>
{{else}}
    <h5>Nenhuma Postagem</h5>
{{/each}}
</br>